{
    "name": "volkmann-design-code/kirby-utils",
    "description": "Utility functions for Kirby Projects",
    "homepage": "https://github.com/volkmann-design-code/kirby-utils",
    "version": "0.10.0",
    "type": "kirby-plugin",
    "scripts": {
        "test": "phpunit tests"
    },
    "require": {
        "getkirby/composer-installer": "^1.2",
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0"
    },
    "require-dev": {
        "getkirby/cms": "~4.1.2 || ~4.6.0 || ~5.0.0",
        "phpunit/phpunit": "^10"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "VolkmannDesignCode\\KirbyUtils\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Enzo Volkmann",
            "email": "enzo@volkmann.dev"
        }
    ],
    "config": {
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    }
}
